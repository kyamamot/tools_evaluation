{
    "volumes": [
        {
            "host": {
                "sourcePath": "/external"
            },
            "name": "scratch"
        }
    ],
    "taskRoleArn": "arn:aws:iam::183726612847:role/ecsInstanceRole",
    "containerDefinitions": [
        {
            "image": "kyamamot/eval-alignment-markdup:20190402",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-stream-prefix": "ecsub",
                    "awslogs-group": "ecsub-create_bams",
                    "awslogs-region": "ap-southeast-1"
                }
            },
            "entryPoint": [
                "/bin/bash",
                "-c"
            ],
            "mountPoints": [
                {
                    "sourceVolume": "scratch",
                    "containerPath": "/scratch"
                }
            ],
            "name": "create_bams_task",
            "workingDirectory": "/scratch",
            "essential": true,
            "environment": [
                {
                    "name": "SCRIPT_EXEC_PATH",
                    "value": "s3://keisuke-nccj-otsuka/tools_evaluation/create_bams/script/script.sh"
                },
                {
                    "name": "SCRIPT_ENVM_PATH",
                    "value": ""
                }
            ],
            "command": [
                "apt update; apt install -y python-pip; pip install awscli --upgrade; aws configure list; aws s3 cp s3://keisuke-nccj-otsuka/tools_evaluation/create_bams/script/run.sh /exec.sh; /bin/bash /exec.sh"
            ],
            "memory": 300,
            "cpu": 1
        }
    ],
    "family": "create_bams"
}